{
  "manifest_version": 3,
  "name": "Test Chrome Extension",
  "version": "1.0.0",
  "description": "test chrome extension with vite,react,ts",
  "action": {
    "default_icon": {
      "16": "icon16-dark.png",
      "24": "icon24-dark.png",
      "32": "icon32-dark.png"
    },
    "default_popup": "index.html"
  },
  "icons": {
    "16": "icon16-dark.png",
    "48": "icon48-dark.png",
    "128": "icon128-dark.png",
    "256": "icon256-dark.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["assets/contentScript-uF3w-Kpz.js"],
      "run_at": "document_start"
    }
  ],
  "background": {
    "service_worker": "assets/background-OK12rsvl.js"
  },
  "permissions": []
}
